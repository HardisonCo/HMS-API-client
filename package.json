{
  "name": "wizard-api-client",
  "version": "1.0.0",
  "description": "TypeScript client library for HMS API with Five-Step Wizard integration",
  "main": "dist/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*",
    "README.md"
  ],
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "npm run build:lib && npm run build:vue",
    "build:lib": "npm run build:cjs && npm run build:esm",
    "build:cjs": "tsc -p tsconfig.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:vue": "vite build",
    "dev": "vite",
    "preview": "vite preview",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "prepublishOnly": "npm run build",
    "generate": "openapi-generator-cli generate -i ../public/docs/api-spec.json -g typescript-axios -o ./src/generated -c ../config/openapi/typescript-config.json",
    "generate:types": "openapi-typescript ../public/docs/api-spec.json -o ./src/generated/api-types.ts"
  },
  "keywords": [
    "api",
    "client",
    "typescript",
    "hms",
    "wizard",
    "five-step-wizard",
    "healthcare",
    "education"
  ],
  "author": "HMS Team",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.0",
    "zod": "^3.23.0",
    "vue": "^3.4.0",
    "pinia": "^2.1.7",
    "vue-router": "^4.2.5",
    "@vueuse/core": "^10.5.0",
    "@vueuse/integrations": "^10.5.0"
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^2.7.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.10.0",
    "@vitejs/plugin-vue": "^4.5.0",
    "@vue/test-utils": "^2.4.1",
    "@vue/tsconfig": "^0.4.0",
    "jest": "^29.7.0",
    "openapi-typescript": "^6.7.0",
    "openapi-typescript-codegen": "^0.25.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0",
    "vitest": "^1.0.0",
    "vue-tsc": "^1.8.0"
  },
  "peerDependencies": {
    "axios": "^1.6.0",
    "vue": "^3.4.0",
    "pinia": "^2.1.0",
    "vue-router": "^4.2.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hms/api-client.git"
  },
  "bugs": {
    "url": "https://github.com/hms/api-client/issues"
  },
  "homepage": "https://github.com/hms/api-client#readme",
  "publishConfig": {
    "access": "public"
  }
}